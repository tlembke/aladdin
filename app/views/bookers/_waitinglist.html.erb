<% waitingArray = Booker.waiting %>
<table class='table'>
	<thead>
		<tr>
			<th>Vaxtype</th>
			<th>Old</th>
			<th>Old - Inv</th>
			<th>New</th>
			<th>New - Inv</th>
		</tr>
	</thead>
	<tbody>
		<% waitingArray.each do |vaxtypeArray| %>
			<tr>
				<% vaxtypeArray.each do |vaxtypeCount| %>
						<td><%= vaxtypeCount %></td>
				<% end %>
			</tr>
		<% end %>
	</tbody>
</table>

