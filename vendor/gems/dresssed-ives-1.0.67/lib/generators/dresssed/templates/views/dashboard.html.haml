    .col-xs-12.col-sm-12.col-md-6.col-lg-6
      .panel.panel-metric
        .panel-body
          .metric-content
            .row
              %header.col-xs-12
                %h3 Monthly Earnings
              .col-xs-7.col-sm-7.col-md-7.col-lg-7{:style => "min-height: 150px;"}
                #metric-monthly-earnings.metric-graph{:style => "height:150px;"}
              .col-xs-5.col-sm-5.col-md-5.col-lg-5
                .metric-value
                  $19,821
                .metric-trend
                  %p.text-success
                    %i.fa.fa-chevron-up
                    12%
                    %small CURRENT
