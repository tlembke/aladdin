.container-fluid.mt20
  .row.top-summary
    .col-lg-3.col-md-6.col-sm-12.animate-drop-fade
      .panel.panel-danger
        .panel-heading
          %p.lead
            %i.fa.fa-globe
            TOTAL
            %b PAGE HITS
        .panel-body.text-red-400
          %h2.page-hits.m0 1123
        .panel-footer
          .row
            .col-sm-12
              %i.fa.fa-caret-down
              %b 10%
              decrease in page hits.
          .clearfix
    .col-lg-3.col-md-6.col-sm-12.animate-drop-fade
      .panel.panel-info
        .panel-heading
          %p.lead
            %i.fa.fa-trophy
            TOTAL
            %b SALES
        .panel-body.text-light-blue-400
          %h2.m0 $12,121
        .panel-footer
          .row
            .col-sm-12
              %i.fa.fa-minus
              %b No change
              in sales
          .clearfix
    .col-lg-3.col-md-6.col-sm-12.animate-drop-fade
      .panel.panel-success
        .panel-heading
          %p.lead
            %i.fa.fa-dollar
            TOTAL
            %b REVENUE
        .panel-body.text-green-400
          %h2.m0 $100,123
        .panel-footer
          .row
            .col-sm-12
              %i.fa.fa-caret-up
              %b 12%
              increase in revenue.
          .clearfix
    .col-lg-3.col-md-6.col-sm-12.animate-drop-fade
      .panel.panel-success
        .panel-heading
          %p.lead
            %i.fa.fa-users
            ACTIVE
            %b CUSTOMERS
        .panel-body.text-green-400
          %h2.m0 23,453
        .panel-footer
          .row
            .col-sm-12
              %i.fa.fa-caret-up
              %b 1%
              increase in active customers.
          .clearfix
